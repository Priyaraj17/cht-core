<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>RDToolkit Demo</h:title>

    <model>
      <instance>
        <rdtoolkitprovisioning delimiter="#" id="rdtoolkit_provisioning_demo">
          <rdtoolkit_patient_id/>
          <rdtoolkit_session_id/>
          <rdtoolkit_state/>
          <rdtoolkit_time_started/>
          <rdtoolkit_time_resolved/>
          <meta>
            <instanceID/>
          </meta>
        </rdtoolkitprovisioning>
      </instance>

      <bind nodeset="/rdtoolkit_provisioning/rdtoolkit_patient_id" type="string"/>
      <bind nodeset="/rdtoolkit_provisioning/rdtoolkit_session_id" type="string"/>
      <bind nodeset="/rdtoolkit_provisioning/rdtoolkit_state" type="string"/>
      <bind nodeset="/rdtoolkit_provisioning/rdtoolkit_time_started" type="string"/>
      <bind nodeset="/rdtoolkit_provisioning/rdtoolkit_time_resolved" type="string"/>
    </model>
  </h:head>

  <h:body class="pages">
    <group appearance="field-list rdtoolkit_provision" ref="/rdtoolkit_provisioning">
      <label>Provision a RD test</label>

      <input ref="/rdtoolkit_provisioning/rdtoolkit_patient_id" appearance="rdtoolkit_patient_id">
        <label>Patient ID</label>
      </input>
      <input ref="/rdtoolkit_provisioning/rdtoolkit_session_id" appearance="rdtoolkit_session_id hidden">
        <label>Session ID</label>
      </input>
      <input ref="/rdtoolkit_provisioning/rdtoolkit_state" appearance="rdtoolkit_state hidden">
        <label>Session ID</label>
      </input>
      <input ref="/rdtoolkit_provisioning/rdtoolkit_time_started" appearance="rdtoolkit_time_started hidden">
        <label>Session ID</label>
      </input>
      <input ref="/rdtoolkit_provisioning/rdtoolkit_time_resolved" appearance="rdtoolkit_time_resolved hidden">
        <label>Session ID</label>
      </input>
    </group>
  </h:body>
</h:html>
